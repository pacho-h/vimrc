{
  "configurations": {
    "Python-vimspector: Launch": {
      "adapter": "debugpy",
      "filetypes": [ "python" ],
      "configuration": {
        "name": "Python-vimspector: Launch",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "python": "<${poetry env info -p}/bin/python>",
        "stopOnEntry": true,
        "console": "externalTerminal",
        "debugOptions": ["-i"],
        "program": "${file}"
      }
    }
  }
}
